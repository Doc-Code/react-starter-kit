{
  "name": "react-mobx-starter-kit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel server:dev lint:watch",
    "build": "npm-run-all remove-dist lint vendor:prod create:dist",
    "start:prod": "npm-run-all remove-dist lint create:dist server:prod",
    "server:dev": "nodemon ./script/server.js --ignore dist --ignore src --ignore node_modules --ignore test --ignore views",
    "server:prod": "cross-env NODE_ENV=production node ./script/server.js",
    "create:dist": "cross-env NODE_ENV=production node ./script/build.js",
    "lint": "esw src",
    "lint:watch": "npm run lint -- --watch",
    "remove-dist": "rimraf ./dist",
    "vendor": "webpack --config ./config/webpack.dll.js",
    "vendor:prod": "cross-env NODE_ENV=production webpack --config ./config/webpack.dll.js"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.0.1",
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^3.0.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.4",
    "ipv4": "^1.0.0",
    "jade": "^1.11.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "morgan": "^1.8.1",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "ora": "^1.2.0",
    "postcss-loader": "^1.3.3",
    "postcss-pxtorem": "^4.0.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "serve-favicon": "^2.4.2",
    "string-loader": "^0.0.1",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.9",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-node-externals": "^1.6.0",
    "webpack-uglify-parallel": "^0.1.3"
  },
  "dependencies": {
    "antd-mobile": "^2.0.3",
    "core-decorators": "^0.17.0",
    "fastclick": "^1.0.6",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.1.4",
    "lodash": "^4.17.4",
    "mobx": "^3.2.2",
    "mobx-logger": "^0.6.0",
    "mobx-react": "^4.2.2",
    "mobx-utils": "^3.1.1",
    "prop-types": "^15.5.10",
    "qs": "^6.5.0",
    "rc-form": "^1.4.6",
    "react": "15",
    "react-dom": "15",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  }
}
